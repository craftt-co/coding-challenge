{
  "compilerOptions": {
    "outDir": "./built",
    "allowJs": true,
    "module": "commonjs",
    "target": "es2022",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
  },
  "lib": [
    "ES2022"
  ],
  "include": [
    "src/**/*",
  ]
}